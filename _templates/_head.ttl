<link text/css "media/myblog.css" rel="stylesheet">
<link text/css "media/pygments.css" rel="stylesheet">
<title> ${title}

@for src in site.siteconfig['google_webfonts'] :
  <link text/css "${src}" rel='stylesheet'>

<style text/css>
  @@ s = '; '.join([ n+':'+v for n,v in site.siteconfig['style'].items() ])
  body { ${s} }
