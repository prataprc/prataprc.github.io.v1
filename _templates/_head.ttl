<!-- jQuery -->
<script "http://ajax.googleapis.com/ajax/libs/jquery/1.10.1/jquery.min.js">

<!-- local media files -->
<link text/css "media/myblog.css" rel="stylesheet">
<link text/css "media/pygments.css" rel="stylesheet">
<!-- Magnific Popup core CSS file -->
<link text/css rel="stylesheet" "media/magnific-popup/magnific-popup.css"> 
<!-- Magnific Popup core JS file -->
<script "media/magnific-popup/jquery.magnific-popup.min.js">

<title> ${title}

@for src in google_webfonts :
